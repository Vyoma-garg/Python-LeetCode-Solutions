class Solution:
    def isPalindrome(self, s: str) -> bool:
        s=s.lower()
        #s=re.sub('[^a-z0-9]', '', s)
        #start=0
        #end=len(s)-1
        #for i in range(len(s)//2):
         #   if s[start]!=s[end]:
          #      return False
           # else:
            #    pass
            #start+=1
            #end-=1
    
        #return True
        
        
        j=''
        for e in s:
            if e.isalnum():
                #j=j+''.join(e)
                 j=j+e

        if (j==j[::-1]):
             return True
        else:
            return False
        
